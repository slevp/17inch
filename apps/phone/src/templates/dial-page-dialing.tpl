<div class="dialing-info">
    <div class="title"><span class="number">{{=number}}</span><span class="connecting">连接中...</span></div>
    <div class="layers">
        <div class="layer outer-bigger"></div>
        <div class="layer outer"></div>
        <div class="layer inner"></div>
        <img class="layer avatar" src="{{=avatar}}"/>
        <div class="layer inner-center">
            <p class="name">{{=name}}</p>
        </div>
    </div>
    <div class="attribution">{{=attribution}}</div>
</div>
<div class="hang-up-btn"></div>
