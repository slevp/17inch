<div class="dial-pad">
    <div class="dial-top">
        <div class="dial-number"></div>
        <div class="dial-delete"></div>
        <div class="dial-cursor">_</div>
    </div>
    <div class="dial-btns">
        <div class="numbers">
            <div class="number1" data-value="1"></div>
            <div class="number2" data-value="2"></div>
            <div class="number3" data-value="3"></div>
            <div class="number4" data-value="4"></div>
            <div class="number5" data-value="5"></div>
            <div class="number6" data-value="6"></div>
            <div class="number7" data-value="7"></div>
            <div class="number8" data-value="8"></div>
            <div class="number9" data-value="9"></div>
            <div class="number-mi" data-value="*"></div>
            <div class="number0" data-value="0"></div>
            <div class="number-jin" data-value="#"></div>
        </div>
        <div class="dialing"></div>
    </div>
</div>
<div class="dialing-pad">
</div>