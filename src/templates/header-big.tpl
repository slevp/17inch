<div class="top-veins"><p>上海億凯信息技术有限公司</p></div>
<div class="voice-btn"></div>
<div class="header-clock">
    <div class="hour-ten"></div>
    <div class="hour-bit"></div>
    <div class="dot"></div>
    <div class="min-ten"></div>
    <div class="min-bit"></div>
</div>
<div class="header-safe-distance">
    <div class="top left-top"></div>
    <div class="middle left-middle"></div>
    <div class="bottom left-bottom"></div>

    <div class="top center-top"></div>
    <div class="middle center-middle"></div>
    <div class="bottom center-bottom"></div>

    <div class="top right-top"></div>
    <div class="middle right-middle"></div>
    <div class="bottom right-bottom"></div>
</div>
<div class="car-container">
    <div class="car"></div>
</div>
<div class="wheel-press"></div>
<div class="big-info">
    <div class="maintain">
        <div class="thousand number6"></div>
        <div class="hundred number5"></div>
        <div class="ten number3"></div>
        <div class="bit number7"></div>
        <div class="dot"></div>
        <div class="decimal number5"></div>
    </div>
    <div class="drive">
        <div class="ten-thousand number2"></div>
        <div class="thousand number3"></div>
        <div class="hundred number5"></div>
        <div class="ten number7"></div>
        <div class="bit number2"></div>
        <div class="dot"></div>
        <div class="decimal number5"></div>
    </div>
    <div class="weather">
        <img class="icon" src="{{=weather.iconPath}}"/>
        <div class="data">
            <p class="temperature"><span class="value">{{=weather.temperature.from}}</span><span class="unit">℃</span></p>
            <p class="status">{{=weather.status}}</p>
        </div>
    </div>
</div>
<div class="alarms">
    <div class="left">
        <div class="icon front-fog"></div>
        <div class="icon stop"></div>
        <div class="icon big-light"></div>
    </div>
    <div class="right">
        <div class="icon key"></div>
        <div class="icon abs"></div>
        <div class="icon back-fog"></div>
    </div>
</div>