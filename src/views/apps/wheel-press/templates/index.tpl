    <div class="data"><span class="value">{{=backLeft}}</span><span class="unit">Kpa</span></div>
    <div class="line"></div>
</div>
<div id="back-right-wheel" class="tip back right">
    <div class="data"><span class="value">{{=backRight}}</span><span class="unit">Kpa</span></div>
    <div class="line"></div>
</div>